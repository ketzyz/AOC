#include <stdio.h>
#include <stdlib.h>

int main() {
    int num1;
    int num2;
    int resultado;

    // Entrada
    printf("Digite o num1: ");
    scanf("%d", &num1); 
    printf("Digite o num2: ");
    scanf("%d", &num2); 

    // Processamento
    resultado = num1 + num2; 

    // Saída
    printf("A soma é %d\n", resultado); 

    return 0; 
}
